<h1>Login To Your Account!</h1>

<%= form_for(@user, url: sessions_path(@user)) do |f| %>

  <div class="form_fields">
    <%= f.label :email %>
      <%= f.email_field :email %>
  </div>

  <div class="form_fields">
    <%= f.label :password %>
      <%= f.password_field :password %>
  </div>

  <%= f.submit "Login" %>

    <% end %>